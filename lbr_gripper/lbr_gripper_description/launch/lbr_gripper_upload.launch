<?xml version="1.0"?>
<launch>
  
  <!-- This lauch file just loads the URDF with the given hardware interface and robot name into the ROS Parameter Server -->
  <arg name="hardware_interface" default="PositionJointInterface"/> 
  <arg name="robot_name" default="lbr"/>
  <arg name="gripper_name" default="gripper"/>
  
  <param name="robot_description" command="$(find xacro)/xacro --inorder '$(find lbr_gripper_description)/urdf/lbr_gripper.urdf.xacro' hardware_interface:=$(arg hardware_interface) robot_name:=$(arg robot_name) gripper_name:=$(arg gripper_name)" />
  
</launch>
